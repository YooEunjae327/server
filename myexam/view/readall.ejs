<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
    </head>

    <body>
        <table>
        <tbody>
            <th>이메일</th>
            <th>이름</th>
            <th>패스워드</th>
            <th>학년</th>
            <th>학반</th>
            <th>PRI key</th>
            <% for(let i in student) { %>
            <th>
                <td><%= student[i].st_id %></td>
                <td><%= student[i].st_name %></td>
                <td><%= student[i].st_pw %></td>
                <td><%= student[i].st_grade %></td>
                <td><%= student[i].st_class %></td>
                <td><%= student[i].st_num %></td>
            </th>
            <% } %>
        </tbody>
        </table>
    </body>
</html>